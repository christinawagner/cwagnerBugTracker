@model IEnumerable<cwagnerBugTracker.Models.Project>

@{
    ViewBag.Title = "ProjectList";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="card">
    <div class="card-body">
        <h4 class="card-title">Project List For All Users</h4>
        @Html.Action("ListProjectsPartial", new { projects = Model })
    </div>
</div>

@section Scripts
{
    <script>
        $(document).ready(function () {
            $('#projectsTable').DataTable({
                responsive: true
            });
        });
    </script>
}
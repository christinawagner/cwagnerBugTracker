@model IEnumerable<cwagnerBugTracker.Models.Project>

@{
    ViewBag.Title = "Index";
}

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Projects</h4>
                @Html.Action("ListProjectsPartial", new { projects = Model })
            </div>
        </div>
    </div>
</div>

@section Scripts
{
    <script>
        $(document).ready(function () {
            $('#projectsTable').DataTable({
                responsive: true
            });
        });
    </script>
}